{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["vue", "prettier"],
    "extends": ["eslint:recommended", "plugin:vue/recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },

    "rules": {
        "vue/html-self-closing": "off",
        "vue/html-indent": "off"
    }
}
